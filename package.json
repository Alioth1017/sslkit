{
  "name": "sslkit",
  "version": "0.0.1",
  "description": "A tool to convert and manage your SSL certificates efficiently.",
  "main": "dist/library.umd.js",
  "module": "dist/library.es.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sslkit": "dist/cli.js"
  },
  "scripts": {
    "build": "vite build && tsup && tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "test": "sslkit -d certificate"
  },
  "keywords": [
    "note-taking",
    "productivity"
  ],
  "author": "Alioth",
  "license": "ISC",
  "files": [
    "dist/**/*",
    "!dist/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "np": "^10.0.5",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.13"
  }
}
