{
  "name": "sslkit",
  "version": "1.0.2",
  "description": "A tool to convert and manage your SSL certificates efficiently.",
  "main": "dist/library.umd.js",
  "module": "dist/library.es.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sslkit": "dist/cli.js"
  },
  "scripts": {
    "build": "rollup -c && tsup && tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "test1": "dist/cli.js -m pfx -d certificate/pfx1",
    "test2": "dist/cli.js -m pfx -d certificate/pfx2"
  },
  "keywords": [
    "note-taking",
    "productivity"
  ],
  "author": "Alioth",
  "license": "ISC",
  "files": [
    "dist/**/*",
    "!dist/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.2",
    "np": "^10.0.7",
    "rollup": "^4.19.2",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4"
  }
}
